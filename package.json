{
  "name": "algorithmx-demo",
  "version": "1.0.0",
  "description": "AlgorithmX online demo.",
  "scripts": {
    "build:dev": "npx webpack --env.env=dev",
    "build:dev:watch": "npm run build:dev -- --watch",
    "serve:dev": "npx webpack-dev-server --env.env=dev --hot",
    "build:prod": "npx webpack -p --env.env=prod",
    "serve:prod": "npm run build:prod && live-server ./public",
    "lint": "npx tslint --project .",
    "typecheck": "npx tsc --noEmit",
    "start": "npm run serve:dev",
    "test": "npm run typecheck && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algrx/algotihmx-demo.git"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@babel/standalone": "7.9.5",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@types/react-redux": "7.1.7",
    "algorithmx": "^1.1.2",
    "brace": "0.11.1",
    "jsnetworkx": "0.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-split": "2.0.7",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@types/node": "13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.2.0",
    "node-sass": "4.14.0",
    "raw-loader": "4.0.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.0",
    "ts-loader": "7.0.1",
    "tslint": "6.1.1",
    "tslint-immutable": "6.0.1",
    "typescript": "3.8.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-concat-plugin": "3.0.0",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  }
