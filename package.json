{
  "name": "algorithmx-demo",
  "version": "1.0.0",
  "description": "AlgorithmX online demo.",
  "scripts": {
    "build:dev": "npx webpack --env.env=dev",
    "build:dev:watch": "npm run build:dev -- --watch",
    "serve:dev": "npx webpack-dev-server --env.env=dev --hot",
    "build:prod": "npx webpack -p --env.env=prod",
    "serve:prod": "npm run build:prod && live-server ./public",
    "lint": "npx tslint --project .",
    "typecheck": "npx tsc --noEmit",
    "start": "npm run serve:dev",
    "test": "npm run typecheck && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algrx/algotihmx-demo.git"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@babel/standalone": "7.4.2",
    "@fortawesome/fontawesome-svg-core": "1.2.16",
    "@fortawesome/free-brands-svg-icons": "5.8.0",
    "@fortawesome/free-regular-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@types/react-redux": "7.0.1",
    "algorithmx": "^1.1.2",
    "brace": "0.11.1",
    "jsnetworkx": "0.3.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "6.0.1",
    "react-split": "2.0.4",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@types/node": "11.10.5",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.0",
    "raw-loader": "2.0.0",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "ts-loader": "5.3.3",
    "tslint": "5.14.0",
    "tslint-immutable": "5.5.0",
    "typescript": "3.3.3333",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-concat-plugin": "3.0.0",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.1.2"
  }
